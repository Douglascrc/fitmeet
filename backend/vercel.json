{
  "version": 2,
  "outputDirectory": "dist",
  "builds": [{ "src": "dist/index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/(.*)", "dest": "dist/index.js" },

    {
      "src": "/(.*)",
      "status": 301,
      "headers": {
        "Location": "https://fitmeet.vercel.app/"
      }
    }
  ]
}

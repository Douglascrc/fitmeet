{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "status": 301,
      "headers": {
        "Location": "https://fitmeet.vercel.app/$1"
      }
    }
  ]
}

{
  "version": 2,
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "^/users(?:/.*)?$", "dest": "dist/index.js" },
    { "src": "^/activities(?:/.*)?$", "dest": "dist/index.js" },
    { "src": "^/auth(?:/.*)?$", "dest": "dist/index.js" },
    {
      "src": "/(.*)",
      "status": 301,
      "headers": {
        "Location": "https://fitmeet.vercel.app/$1"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
